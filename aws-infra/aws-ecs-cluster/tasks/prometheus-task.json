[
  {
    "Image": "${image}",
    "LogConfiguration": {
      "LogDriver": "awslogs",
      "Options": {
        "awslogs-group": "${log_group}",
        "awslogs-stream-prefix": "monitoring-app",
        "awslogs-region": "${aws_region}"
      }
    },
    "Memory": "${memory}",
    "Name": "Prometheus-monitoring",
    "PortMappings": [
      {
        "ContainerPort": 9090,
        "HostPort": 9090,
        "Protocol": "tcp"
      }
    ]
  }
]